{
  "name": "react-starter",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Nikoloz Buligini",
  "scripts": {
    "start": "cross-env NODE_ENV=development babel-node ./config/scripts/start-dev",
    "bundle": "rimraf dist && babel-node config/scripts/build",
    "build": "yarn bundle -- --analyzer=true",
    "serve": "cross-env NODE_ENV=production cross-env PORT=8000 babel-node ./server",
    "deploy": "yarn bundle && babel-node config/scripts/deploy",
    "deploy:test": "yarn deploy -- --target=test",
    "deploy:production": "yarn deploy -- --target=production",
    "lint": "eslint . --ext .jsx,.js",
    "flow:build": "flow-typed install",
    "flow": "flow check",
    "test": "jest",
    "test:watch": "yarn test -- --watch",
    "postinstall": "yarn flow:build"
  },
  "dependencies": {
    "compression": "^1.7.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-helmet": "^5.2.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "styled-components": "^2.1.2",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
