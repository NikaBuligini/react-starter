{
  "name": "react-starter",
  "version": "0.0.1",
  "main": "n/a",
  "author": "Nikoloz Buligini",
  "engines": {
    "node": ">=8.8.0"
  },
  "scripts": {
    "build": "yarn bundle --analyzer",
    "build:prod": "yarn bundle:clusters --port=80",
    "bundle": "yarn bundle:clean && node config/scripts/start-build",
    "bundle:clusters": "node config/scripts/start-build --clusters",
    "bundle:clean": "yarn clean --target=dist",
    "browse": "gcloud app browse",
    "clean": "node ./config/scripts/clean",
    "clean:all": "yarn bundle:clean && yarn flow:clean && yarn test:clean",
    "check:all": "yarn prettier:diff && yarn lint && yarn flow",
    "deploy": "yarn prepare-deploy && gcloud app deploy --stop-previous-version",
    "flow": "node ./config/scripts/flow",
    "flow:build": "yarn flow:clean && flow-typed install",
    "flow:clean": "yarn clean --target=flow",
    "lint": "node ./config/scripts/eslint",
    "precommit": "lint-staged",
    "prepare-deploy": "yarn && yarn bundle",
    "prettier": "prettier --write \"{config,server,src}/**/*.{js,json,css,md}\"",
    "prettier:diff": "prettier --list-different \"{config,server,src}/**/*.{js,json,css,md}\"",
    "start": "cross-env NODE_ENV=production node ./dist/main/server",
    "start:dev": "yarn bundle:clean && node ./config/scripts/run-dev",
    "test": "node ./config/scripts/start-test --env=jsdom",
    "test:coverage": "yarn test:clean && yarn test --coverage",
    "test:verbose": "yarn test --verbose",
    "test:watch": "yarn test --watch",
    "test:clean": "yarn clean --target=coverage"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "compression": "^1.7.3",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.1",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "ip": "^1.1.5",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.2.0",
    "localforage": "^1.7.2",
    "lodash": "^4.17.10",
    "method-override": "^3.0.0",
    "moment": "^2.22.2",
    "normalizr": "^3.2.4",
    "object-assign": "^4.1.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-redux-progress": "^1.0.1",
    "react-router-dom": "^4.3.1",
    "redis": "^2.8.0",
    "redux": "^4.0.0",
    "redux-persist": "^5.4.0",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.12.0",
    "response-time": "^2.3.2",
    "serve-favicon": "^2.5.0",
    "statuses": "^1.5.0",
    "styled-components": "^3.3.3",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^2.0.4",
    "winston": "^3.0.0",
    "winston-daily-rotate-file": "^3.3.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^4.5.2",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.1.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.7.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.10.0",
    "exists-sync": "^0.1.0",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "flow-bin": "^0.76.0",
    "flow-typed": "^2.5.1",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy": "^1.17.0",
    "jest": "^23.4.1",
    "jsdom": "^11.11.0",
    "keypress": "^0.2.1",
    "lint-staged": "^7.2.0",
    "nock": "^9.4.2",
    "node-sass": "^4.9.2",
    "nodemon": "^1.18.3",
    "npmlog": "^4.1.2",
    "prettier": "^1.13.7",
    "pretty-error": "^2.1.1",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "sinon": "^6.1.3",
    "sitemap-webpack-plugin": "^0.6.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-server": "^3.1.4",
    "yargs": "^12.0.1"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "<rootDir>/src/testsSetup.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
