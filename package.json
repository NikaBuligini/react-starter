{
  "name": "react-starter",
  "version": "0.0.1",
  "main": "index.js",
  "author": "Nikoloz Buligini",
  "engines": {
    "node": "8.x"
  },
  "scripts": {
    "start": "npm run serve",
    "start:dev": "cross-env NODE_ENV=development babel-node ./config/scripts/start-dev",
    "bundle": "node config/scripts/start-build",
    "build": "yarn bundle --analyzer=true",
    "build:prod": "yarn bundle --production --port=80 && ",
    "serve": "node ./dist/main/server",
    "deploy": "yarn bundle && babel-node config/scripts/deploy",
    "deploy:test": "yarn deploy --target=test",
    "deploy:production": "yarn deploy --target=production",
    "lint": "babel-node ./config/scripts/eslint",
    "flow:build": "flow-typed install",
    "flow": "babel-node ./config/scripts/flow",
    "test": "jest",
    "test:coverage": "yarn test --coverage --collectCoverageFrom=**/src/*.{js,jsx}",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "compression": "^1.7.0",
    "connect-redis": "^3.3.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "helmet": "^3.8.1",
    "history": "^4.7.2",
    "humps": "^2.0.1",
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "normalizr": "^3.2.3",
    "prop-types": "^15.5.10",
    "react": "^16.0.0-rc.3",
    "react-dom": "^16.0.0-rc.3",
    "react-helmet": "^5.2.0",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redis": "^2.8.0",
    "redux": "^3.7.2",
    "redux-persist": "^4.9.1",
    "redux-thunk": "^2.2.0",
    "response-time": "^2.3.2",
    "serve-favicon": "^2.4.3",
    "styled-components": "^2.1.2",
    "uuid": "^3.1.0",
    "whatwg-fetch": "^2.0.3",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-styled-components": "^1.2.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.24.1",
    "chalk": "^2.1.0",
    "compression-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-flowtype": "^2.35.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "exists-sync": "^0.0.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "flow-bin": "^0.54.1",
    "flow-typed": "^2.1.5",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy": "^1.16.2",
    "jest": "^21.0.1",
